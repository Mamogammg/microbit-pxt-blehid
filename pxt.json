{
    "name": "microbit-pxt-blehid",
    "version": "0.0.0",
    "description": "BLE HID Support",
    "license": "MIT",
    "dependencies": {
        "core": "*",
        "bluetooth": "*"
    },
    "files": [
        "README.md",
        "HIDService.cpp",
        "HIDService.h",
        "debug.h",
        "shims.d.ts",
        "enums.d.ts",
        "main.py",
        "ascii2scan.cpp",
        "ascii2scan.h", 
        "blekeyboard.ts", 
        "blekeyboard.cpp",
        "BLEKeyboardService.cpp",
        "BLEKeyboardService.h", 
        "blemouse.ts", 
        "blemouse.cpp",
        "BLEMouseService.cpp",
        "BLEMouseService.h"
    ],
    "testFiles": [
        "test.ts"
    ],
    "public": true,
    "targetVersions": {
        "target": "4.1.9",
        "targetId": "microbit"
    },
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "disablesVariants": [
        "mbdal"
    ],
    "authors": [
        "Bill Siever <bsiever@gmail.com>"
    ],
    "card": {
        "name": "BLE HID",
        "feedbackUrl": "http://siever.info"
    },
    "yotta": {
        "config": {
            "DEVICE_DMESG_BUFFER_SIZE": 8192,
            "DMESG_SERIAL_DEBUG": 1,
            "microbit-dal": {
                "bluetooth": {
                    "open": 0,
                    "whitelist": 1,
                    "security_level": "SECURITY_MODE_ENCRYPTION_NO_MITM",
                    "device_info_service": 0
                }
            }
        }
    }
}
